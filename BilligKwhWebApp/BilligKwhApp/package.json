    "@types/jasminewd2": "~2.0.8",
    "@types/jquery": "~3.5.13",
    "@types/node": "^12.11.1",
    "@types/pickadate": "~3.5.32",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
