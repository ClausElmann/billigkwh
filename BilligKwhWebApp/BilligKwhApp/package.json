    "ng": "ng",
    "start": "ng serve --hmr --port 4202 --disable-host-check",
    "openapi": "SET JAVA_OPTS=-Dio.swagger.parser.util.RemoteUrl.trustAll=true -Dio.swagger.v3.parser.util.RemoteUrl.trustAll=true && openapi-generator-cli generate -i https://localhost:44321/swagger/v1/swagger.json -g typescript-angular -o ./src/code-gen --global-property models",
