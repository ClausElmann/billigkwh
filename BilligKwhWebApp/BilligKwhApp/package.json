  "private": true,
  "scripts": {
    "build": "ng build --output-hashing=all",
    "generate-client-sdk": "mkdir src/libs/api-client && openapi-generator-cli generate -i https://localhost/swagger/v1/swagger.json -g typescript-angular -o src/libs/api-client",
