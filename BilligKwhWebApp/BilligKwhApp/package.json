{
  "name": "billigkwh-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "ng build --output-hashing=all",
    "generate-client-sdk": "mkdir src/libs/api-client && openapi-generator-cli generate -i https://localhost/swagger/v1/swagger.json -g typescript-angular -o src/libs/api-client",
    "lint": "eslint \"**/*.ts\" --quiet --fix",
    "ng": "ng",
    "start": "ng serve --hmr --port 4202 --disableHostCheck=true",
    "openapi": "SET JAVA_OPTS=-Dio.swagger.parser.util.RemoteUrl.trustAll=true -Dio.swagger.v3.parser.util.RemoteUrl.trustAll=true && openapi-generator-cli generate -i https://localhost:44321/swagger/v1/swagger.json -g typescript-angular -o ./src/code-gen --global-property models",
    "test": "ng test",
    "watch": "ng build --watch --configuration development"
  },
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/cdk": "~13.0.0",
    "@angular/common": "~13.0.0",
